import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int[] arr = new int[n];
        for (int i = 0; i < n; i++) {
            arr[i] = sc.nextInt();
        }
        int k = sc.nextInt();
        for (int i = 0; i <= n - k; i++) {
            int min = arr[i];
            for (int j = i + 1; j < i + k; j++) {
                if (arr[j] < min) {
                    min = arr[j];
                }
            }
            System.out.print(min + " ");
        }
    }
}